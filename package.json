  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.7",
    "@types/nearley": "^2.11.2",
    "@types/node": "^22.5.0",
    "glob": "^11.0.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5"
  }
